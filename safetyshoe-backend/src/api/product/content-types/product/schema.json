{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": "产品信息 (New Standard)"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "model_code": {
      "type": "string",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "description": {
      "type": "text",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "images": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "moq": {
      "type": "string",
      "default": "500 Pairs",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "safety_standard": {
      "type": "enumeration",
      "enum": [
        "SB",
        "S1",
        "S1P",
        "S2",
        "S3",
        "OB"
      ],
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "additional_certs": {
      "type": "json",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "style": {
      "type": "enumeration",
      "enum": [
        "Low Cut",
        "Mid Cut",
        "High Boot",
        "Sandal",
        "Sporty"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "industries": {
      "type": "json",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "materials": {
      "type": "component",
      "repeatable": false,
      "component": "product.material-spec",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "features": {
      "type": "json",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "is_hot": {
      "type": "boolean",
      "default": false,
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "is_new": {
      "type": "boolean",
      "default": false,
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "price_range": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    }
  }
}