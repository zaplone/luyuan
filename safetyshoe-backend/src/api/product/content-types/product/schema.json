{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": "产品信息"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "sku": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "description": {
      "type": "text",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "images": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "moq": {
      "type": "integer",
      "default": 500,
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "category": {
      "type": "enumeration",
      "enum": [
        "Construction",
        "Oil & Gas",
        "Manufacturing",
        "Logistics",
        "Mining",
        "Food Industry",
        "Other"
      ],
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "standards": {
      "type": "json",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "features": {
      "type": "json",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "price_range": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "is_hot": {
      "type": "boolean",
      "default": false,
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "is_new": {
      "type": "boolean",
      "default": false,
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "style": {
      "type": "enumeration",
      "enum": [
        "Low Cut",
        "Mid Cut",
        "High Boot"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "materials": {
      "type": "json",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    }
  }
}

